#include<iostream>
using namespace std;
const int Max=5;
class queue
{
private:
int a[Max]; int front; int rear;
public:
queue()
{
front=-1; rear=-1;
}
int empty(void)
{
if(front==-1&&rear==-1)
return(1);
else
return(0);
}
int full(void)
{
if(rear==Max-1)
return(1);
else
return(0);
}
void enqueue(int x)
{
if(front==-1&&rear==-1)
{
front++; rear++;
}
else rear++;
a[rear]=x;
}
int dequeue(void)
{
int x=a[front];
if(front==rear)
{
front=-1; rear=-1;
}
else front++;
return(x);
}
int peek(void)
{
return(a[front]);
}
void display(void){
cout<<endl;
for(int i=front;i<=rear;i++)
cout<<"\t"<<a[i];
}
};
int main()
{
queue q; int x;
if(!q.full())
q.enqueue(10);
if(!q.full())
q.enqueue(20);
if(!q.full())
q.enqueue(30);
if(!q.full())
q.enqueue(40);
if(!q.full())
q.enqueue(50);
if(!q.full())
q.enqueue(60);
q.display();
if(!q.empty())
{
x=q.dequeue();
cout<<endl<<"Removed element:"<<x;
}
cout<<endl<<"After dequeue:";
q.display();
q.peek();
cout<<endl<<"Front element is:"<<x;
return(0);
}
